{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/g5search/messages/blob/master/analytics/calls/v1/calls.schema.json",
  "type": "object",
  "description": "Call payload",
  "required": [
    "call_id",
    "vertical",
    "location_info",
    "client_info",
    "call_payload"
  ],
  "properties": {
    "call_payload": {
      "type": "object",
      "properties": {
        "call_event_urn": {
          "type": "string"
        },
        "location_urn": {
          "type": "string"
        },
        "category": {
          "type": "string"
        },
        "ga_client_id": {
          "type": "string"
        },
        "cpm_pool_cpm_code": {
          "type": "string"
        },
        "cpm_pool_marketing_source": {
          "type": "string"
        },
        "created_at": {
          "type": [
            "string",
            "any"
          ]
        },
        "source": {
          "type": "string"
        },
        "medium": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "tracking_purpose": {
          "type": "string"
        },
        "params": {
          "type": "object"
        }
      },
      "required": [
        "location_urn",
        "category",
        "created_at",
        "params"
      ]
    },
    "call_id": {
      "type": "string"
    },
    "vertical": {
      "type": "string"
    },
    "location_info": {
      "type": "object",
      "properties": {
        "urn": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "postal_code": {
          "type": "string"
        }
      },
      "required": [
        "urn"
      ]
    },
    "client_info": {
      "type": "object",
      "properties": {
        "urn": {
          "type": "string"
        },
        "domain_type": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "postal_code": {
          "type": "string"
        }
      },
      "required": [
        "urn"
      ]
    }
  }
}